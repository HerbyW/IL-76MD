 <instrumentation>

<dme n="0">
    <frequencies>
       <source type="string">/instrumentation/dme[0]/frequencies/selected-mhz</source>
    </frequencies>
    <switch-position n="0" type="int">0</switch-position>
    <switch-position n="1" type="int">0</switch-position>
